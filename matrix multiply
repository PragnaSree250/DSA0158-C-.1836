#include <iostream>
using namespace std;
int main()
{
	int mat1[10][10];
	int mat2[10][10];
	int res[10][10];
	int n,i,j,r,c;
	cout<<"no.of rows in matrix:";
	cin>>r;
	cout<<"no.of col in matrix:";
	cin>>c;
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			cin>>mat1[i][j];
		}
	}
	cout<<"enter the elements of second matrix:"<<endl;
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			cin>>mat2[i][j];
		}
	}
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			res[i][j]=0;
		}
	}
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			for(int k=0;k<c;k++)
			{
			res[i][j]+=mat1[i][k]*mat2[k][j];
			}
		}
	}
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
		  cout<<	res[i][j]<<" ";
		}
		cout<<endl;
	}

return 0;
}
