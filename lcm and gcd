#include <iostream>


int gcd(int a, int b) {
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}
int lcm(int a, int b) {
    return (a * b) / gcd(a, b);
}

int main() {
    int num1, num2;
    std::cout << "Enter first number: ";
    std::cin >> num1;
    std::cout << "Enter second number: ";
    std::cin >> num2;

    int gcdValue = gcd(num1, num2);
    int lcmValue = lcm(num1, num2);

    std::cout << "GCD of " << num1 << " and " << num2 << " is: " << gcdValue << std::endl;
    std::cout << "LCM of " << num1 << " and " << num2 << " is: " << lcmValue << std::endl;

    return 0;
}
