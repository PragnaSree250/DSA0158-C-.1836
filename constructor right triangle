#include<iostream>
using namespace std;
class righttrinum{
	private:
	int n;
	public:
	righttrinum():n(0)
	{

	}
	righttrinum(int num):n(num)
	{
        cout<<n<<endl;
	}
	void display(){
		
		for(int i=1;i<=n;i++)
		{
			for(int j=1;j<=i;j++)
			{
                cout<<j;
				cout<<" ";
			}
			cout<<endl;
		}
		
	}
};
int main()
{
	int num;
	cout<<"num value";
	cin>>num;
	righttrinum obj(num);
	obj.display();
    return 0;

}
