#include<iostream>
using namespace std;
int main()
{
    int n;
    int sum=0;
    cout<<"enter the number value";
    cin>>n;
    for(int i=1;i<n;i++)
    {
        if(n%i==0)
        {
            sum=sum+i;
        }
        
    }
    if(n==sum)
    {
        cout<<"number is perfect";
    }
    else{
        cout<<"not perfect";
    }
    return 0;
}
